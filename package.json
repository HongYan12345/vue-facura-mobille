{
  "name": "vue-factura-mobille",
  "private": true,
  "version": "0.0.4",
  "type": "module",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "dev:electron": "vite",
    "build": "vite build",
    "build:mobile": "vite build --config vite.config.mobile.ts",
    "build:electron": "vite build && electron-builder",
    "build:check": "vue-tsc && vite build",
    "preview": "vite preview",
    "cordova:init": "npx cordova create cordova net.xie.factura XieFactura && cd cordova && npx cordova platform add android",
    "cordova:sync": "node ./scripts/cordova-sync.mjs",
    "cordova:res": "node ./scripts/cordova-res.mjs",
    "cordova:build": "npm run build:mobile && npm run cordova:sync && npm run cordova:res && npx cordova build android",
    "release": "node ./scripts/release.mjs"
  },
  "dependencies": {
    "@electron/remote": "^2.1.3",
    "ant-design-vue": "^3.2.15",
    "firebase": "^9.22.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "sqlite3": "^5.0.2",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "cordova": "^12.0.0",
    "cordova-res": "^0.15.4",
    "electron": "^39.1.1",
    "electron-builder": "^26.0.12",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vue-tsc": "^1.4.2"
  }
}
